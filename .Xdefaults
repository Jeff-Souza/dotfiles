URxvt.transparent: false
! URxvt.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt.shading: 180

URxvt.depth:32
URxvt.inputMethod:ibus
URxvt.scrollBar:False
URxvt.scrollBar_right:False
URxvt.scrollBar_floating: False
URxvt.scrollstyle:plain
URxvt.saveLines:10000
URxvt.menu:/etc/X11/rxvt.menu
URxvt.preeditType:Root
URxvt.jumpScroll:false
URxvt.cursorBlink:true
!Icon
URxvt.iconFile:/usr/share/icons/gnome/16x16/apps/terminal.png
!Set TERM
URxvt.termName:rxvt-unicode-256color
#Tab support
!URxvt.perl-ext-common:  default,tabbed,url-picker,clipboard,matcher
!Shorcut HELP
!Shift+ ? new tab
!Shift+ ? go to left tab
!Shift+ ? go to right tab
!Ctrl+ ? move tab to the left
!Ctrl+ ? move tab to the right
!Ctrl+D: close tab
!URxvt.tabbed.tabbar-fg: 2
!URxvt.tabbed.tabbar-bg: #575757
!URxvt.tabbed.tab-fg:    12
!URxvt.tabbed.tab-bg:    #575757
!URxvt.geometry:300x100
!URxvt*cursorUnderline: true
!URxvt.tabbed.autohide: true


# Transparency
URxvt.transparent:	true
URxvt.shading:		20


Xft.dpi:96
Xft.antialias:			true
Xft.rgba:			rgb
Xft.autohint:			false
Xft.hinting:			true
Xft.hintstyle:			hintfull
urxvt*font:                 	xft:Cousine:Bold:size=10
urxvt*imFont:               	xft:Cousine:Bold:size=10
urxvt*boldFont:             	xft:Cousine:Bold:size=10
urxvt*italicFont:           	xft:Cousine:Bold:size=10
urxvt*boldItalicFont:       	xft:Cousine:bold:italic:size=10
urxvt*letterSpace:	-1

# clickable Url and copy paste keys

URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.iso14755: False

URxvt.url-launcher: /usr/bin/firefox

URxvt.matcher.button: 1


Xft.lcdfilter:  lcddefault



URxvt*dynamicColors:    on
URxvt*highlightSelection: true
URxvt*colorMode:    on



urxvt*borderColor:		#373b41


! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
